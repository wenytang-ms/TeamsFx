    "test:function": "nyc --no-timeouts --require ts-node/register \"tests/plugins/resource/function/**/*.test.ts\"",
    "test:localdebug": "nyc mocha --no-timeouts --require ts-node/register \"tests/plugins/resource/localdebug/**/*.test.ts\"",
